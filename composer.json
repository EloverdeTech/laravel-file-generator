{
    "name": "eloverde/laravel-file-generator",
    "description": "Laravel package to help you automate creation of files. Build your own custom generators like 'artisan make:model'.",
    "type": "library",
    "keywords": [
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.2",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/console": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/view": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/filesystem": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "symfony/finder": "^6.0|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.5"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "autoload": {
        "psr-4": {
            "Skyronic\\FileGenerator\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Skyronic\\FileGenerator\\FileGeneratorServiceProvider"
            ]
        }
    }
}
